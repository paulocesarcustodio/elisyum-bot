{
  "name": "lbot-whatsapp",
  "version": "3.4.8",
  "description": "Bot brasileiro para Whatsapp com diversas utilidades para o dia-a-dia.",
  "type": "module",
  "main": "dist/app.js",
  "packageManager": "bun@1.1.0",
  "scripts": {
    "start": "bun start.js",
    "start:simple": "bun dist/app.js",
    "clean": "rimraf dist/",
    "copy-media": "copyfiles -u 2 src/media/* dist/media",
    "prestart": "bun install",
    "dev": "bun run build && bun start.js",
    "migrate": "cross-env migrate=y bun run dev",
    "build": "bun run clean && bun run tsc && bun run copy-media",
    "zip": "bun run build && bestzip LBOT-v$npm_package_version.zip dist package.json LICENSE bun.lock",
    "tsc": "tsc"
  },
  "author": "Leal",
  "license": "GPL-3.0-only",
  "repository": "git+https://github.com/victorsouzaleal/lbot-whatsapp.git",
